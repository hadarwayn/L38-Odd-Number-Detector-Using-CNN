{
    "ring_buffer": {
        "max_lines_per_file": 1000,
        "max_log_files": 5,
        "log_directory": "logs",
        "log_filename_pattern": "app_{timestamp}.log",
        "log_level": "INFO",
        "format": "%(asctime)s - %(levelname)s - %(name)s - %(message)s",
        "date_format": "%Y-%m-%d %H:%M:%S"
    }
}
